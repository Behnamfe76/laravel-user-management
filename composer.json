{
    "name": "fereydooni/laravel-user-management",
    "description": "Dynamic user management for Laravel with attribute-based field and role definitions",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Fereydooni\\LaravelUserManagement\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fereydooni\\LaravelUserManagement\\Tests\\": "tests/",
            "Fereydooni\\LaravelUserManagement\\Tests\\Models\\": "tests/Models/",
            "Fereydooni\\LaravelUserManagement\\Tests\\Factories\\": "tests/Factories/"
        }
    },
    "authors": [
        {
            "name": "Behnam Fereydooni",
            "email": "101217538+Behnamfe76@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-permission": "^6.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fereydooni\\LaravelUserManagement\\UserManagementServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "analyse": "vendor/bin/phpstan analyse"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "1.0.0"
}
